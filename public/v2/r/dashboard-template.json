      "path": "src/registry/templates/Test/Dashboard/dashboard-template.tsx",
      "content": "\"use client\";\n\nimport React from \"react\";\nimport { Avatar, AvatarFallback, AvatarImage } from \"@/components/ui/avatar\";\nimport { Button } from \"@/components/ui/button\";\nimport { Card, CardContent, CardHeader, CardTitle } from \"@/components/ui/card\";\nimport { Input } from \"@/components/ui/input\";\nimport {\n  Table,\n  TableBody,\n  TableCell,\n  TableHead,\n  TableHeader,\n  TableRow,\n} from \"@/components/ui/table\";\nimport {\n  Activity,\n  ArrowUpRight,\n  CircleUser,\n  CreditCard,\n  DollarSign,\n  Package2,\n  Search,\n  Users,\n} from \"lucide-react\";\n\nexport function DashboardTemplate() {\n  return (\n    <div className=\"flex min-h-screen w-full flex-col\">\n      {/* 🎯 목적: 네비게이션 헤더 - 브랜드 로고, 메뉴, 검색, 사용자 프로필 */}\n      <header className=\"bg-background flex items-center justify-between border-b px-6 py-4\">\n        <nav className=\"flex items-center gap-6\">\n          {/* 브랜드 로고 영역 */}\n          <div className=\"flex items-center gap-2\">\n            <Package2 className=\"text-foreground h-6 w-6\" />\n          </div>\n          {/* 네비게이션 메뉴 */}\n          <div className=\"flex items-center gap-6\">\n            <span className=\"text-foreground text-sm font-medium\">\n              Dashboard\n            </span>\n            <span className=\"text-muted-foreground text-sm\">Orders</span>\n            <span className=\"text-muted-foreground text-sm\">Products</span>\n            <span className=\"text-muted-foreground text-sm\">Customers</span>\n            <span className=\"text-muted-foreground text-sm\">Analytics</span>\n          </div>\n        </nav>\n        {/* 우측 유틸리티 영역 */}\n        <div className=\"flex items-center gap-4\">\n          {/* 검색 입력창 */}\n          <div className=\"relative w-80\">\n            <Search className=\"text-muted-foreground absolute top-1/2 left-3 h-4 w-4 -translate-y-1/2\" />\n            <Input placeholder=\"Search...\" className=\"pl-10\" />\n          </div>\n          {/* 사용자 프로필 버튼 */}\n          <Button variant=\"ghost\" size=\"icon\" className=\"bg-muted rounded-full\">\n            <CircleUser className=\"h-4 w-4\" />\n          </Button>\n        </div>\n      </header>\n\n      {/* 🎯 목적: 메인 콘텐츠 영역 - 대시보드 통계 및 데이터 */}\n      <main className=\"bg-background flex-1 space-y-8 p-8\">\n        {/* 🎯 목적: 통계 카드 그리드 - 수익, 구독, 판매, 활성 사용자 현황 */}\n        <div className=\"grid gap-8 md:grid-cols-2 lg:grid-cols-4\">\n          {/* 총 수익 카드 */}\n          <Card className=\"border\">\n            <CardHeader className=\"flex flex-row items-center justify-between space-y-0 pb-2\">\n              <CardTitle className=\"text-foreground text-sm font-medium\">\n                Total Revenue\n              </CardTitle>\n              <DollarSign className=\"text-muted-foreground h-4 w-4\" />\n            </CardHeader>\n            <CardContent>\n              <div className=\"text-foreground text-2xl font-bold\">\n                $2,131.86\n              </div>\n              <p className=\"text-muted-foreground text-xs\">\n                +20% from last month\n              </p>\n            </CardContent>\n          </Card>\n\n          {/* 구독 카드 */}\n          <Card className=\"border\">\n            <CardHeader className=\"flex flex-row items-center justify-between space-y-0 pb-2\">\n              <CardTitle className=\"text-foreground text-sm font-medium\">\n                Subscriptions\n              </CardTitle>\n              <Users className=\"text-muted-foreground h-4 w-4\" />\n            </CardHeader>\n            <CardContent>\n              <div className=\"text-foreground text-2xl font-bold\">\n                $7,411.30\n              </div>\n              <p className=\"text-muted-foreground text-xs\">\n                +10% from last month\n              </p>\n            </CardContent>\n          </Card>\n\n          {/* 판매 카드 */}\n          <Card className=\"border\">\n            <CardHeader className=\"flex flex-row items-center justify-between space-y-0 pb-2\">\n              <CardTitle className=\"text-foreground text-sm font-medium\">\n                Sales\n              </CardTitle>\n              <CreditCard className=\"text-muted-foreground h-4 w-4\" />\n            </CardHeader>\n            <CardContent>\n              <div className=\"text-foreground text-2xl font-bold\">\n                $9,896.66\n              </div>\n              <p className=\"text-muted-foreground text-xs\">\n                +19% from last month\n              </p>\n            </CardContent>\n          </Card>\n\n          {/* 활성 사용자 카드 */}\n          <Card className=\"border\">\n            <CardHeader className=\"flex flex-row items-center justify-between space-y-0 pb-2\">\n              <CardTitle className=\"text-foreground text-sm font-medium\">\n                Active Now\n              </CardTitle>\n              <Activity className=\"text-muted-foreground h-4 w-4\" />\n            </CardHeader>\n            <CardContent>\n              <div className=\"text-foreground text-2xl font-bold\">\n                $7,940.85\n              </div>\n              <p className=\"text-muted-foreground text-xs\">\n                +201 since last hour\n              </p>\n            </CardContent>\n          </Card>\n        </div>\n\n        {/* 🎯 목적: 콘텐츠 그리드 - 트랜잭션 테이블과 최근 판매 */}\n        <div className=\"grid gap-8 lg:grid-cols-7\">\n          {/* 트랜잭션 테이블 */}\n          <Card className=\"col-span-4 border\">\n            <CardHeader className=\"flex flex-row items-center justify-between space-y-0 pb-6\">\n              <div className=\"space-y-1\">\n                <CardTitle className=\"text-foreground text-xl font-semibold\">\n                  Transactions\n                </CardTitle>\n                <p className=\"text-muted-foreground text-sm\">\n                  Recent transactions from your store.\n                </p>\n              </div>\n              <Button className=\"bg-primary text-primary-foreground hover:bg-primary/90\">\n                View All\n                <ArrowUpRight className=\"ml-2 h-4 w-4\" />\n              </Button>\n            </CardHeader>\n            <CardContent className=\"px-6 pb-6\">\n              <Table>\n                <TableHeader>\n                  <TableRow>\n                    <TableHead className=\"text-foreground text-sm font-medium\">\n                      Customer\n                    </TableHead>\n                    <TableHead className=\"text-foreground text-right text-sm font-medium\">\n                      Amount\n                    </TableHead>\n                  </TableRow>\n                </TableHeader>\n                <TableBody>\n                  {/* 거래 데이터 행들 */}\n                  <TableRow>\n                    <TableCell>\n                      <div className=\"space-y-1\">\n                        <p className=\"text-foreground text-sm font-medium\">\n                          Alex Buckmaster\n                        </p>\n                        <p className=\"text-muted-foreground text-sm\">\n                          alex_b@gmail.com\n                        </p>\n                      </div>\n                    </TableCell>\n                    <TableCell className=\"text-foreground text-right text-sm\">\n                      $4,024.92\n                    </TableCell>\n                  </TableRow>\n                  <TableRow>\n                    <TableCell>\n                      <div className=\"space-y-1\">\n                        <p className=\"text-foreground text-sm font-medium\">\n                          Kathy Pacheco\n                        </p>\n                        <p className=\"text-muted-foreground text-sm\">\n                          kathy_pac@gmail.com\n                        </p>\n                      </div>\n                    </TableCell>\n                    <TableCell className=\"text-foreground text-right text-sm\">\n                      $9,952.52\n                    </TableCell>\n                  </TableRow>\n                  <TableRow>\n                    <TableCell>\n                      <div className=\"space-y-1\">\n                        <p className=\"text-foreground text-sm font-medium\">\n                          Lorri Warf\n                        </p>\n                        <p className=\"text-muted-foreground text-sm\">\n                          lorri@aol.com\n                        </p>\n                      </div>\n                    </TableCell>\n                    <TableCell className=\"text-foreground text-right text-sm\">\n                      $3,153.64\n                    </TableCell>\n                  </TableRow>\n                  <TableRow>\n                    <TableCell>\n                      <div className=\"space-y-1\">\n                        <p className=\"text-foreground text-sm font-medium\">\n                          James Hall\n                        </p>\n                        <p className=\"text-muted-foreground text-sm\">\n                          hall45@outlook.com\n                        </p>\n                      </div>\n                    </TableCell>\n                    <TableCell className=\"text-foreground text-right text-sm\">\n                      $7,369.55\n                    </TableCell>\n                  </TableRow>\n                  <TableRow className=\"border-b-0\">\n                    <TableCell>\n                      <div className=\"space-y-1\">\n                        <p className=\"text-foreground text-sm font-medium\">\n                          Chris Glasser\n                        </p>\n                        <p className=\"text-muted-foreground text-sm\">\n                          chris_glass@aol.com\n                        </p>\n                      </div>\n                    </TableCell>\n                    <TableCell className=\"text-foreground text-right text-sm\">\n                      $2,192.86\n                    </TableCell>\n                  </TableRow>\n                </TableBody>\n              </Table>\n            </CardContent>\n          </Card>\n\n          {/* 최근 판매 */}\n          <Card className=\"col-span-3 border\">\n            <CardHeader>\n              <CardTitle className=\"text-foreground text-xl font-semibold\">\n                Recent Sales\n              </CardTitle>\n            </CardHeader>\n            <CardContent className=\"space-y-8\">\n              <div className=\"flex items-center space-x-4\">\n                <Avatar className=\"h-12 w-12\">\n                  <AvatarImage src=\"data:image/png;base64,\" />\n                  <AvatarFallback>SN</AvatarFallback>\n                </Avatar>\n                <div className=\"flex-1 space-y-1\">\n                  <p className=\"text-foreground text-sm font-medium\">\n                    Stephanie Nicol\n                  </p>\n                  <p className=\"text-muted-foreground text-sm\">\n                    steph56@gmail.com\n                  </p>\n                </div>\n                <div className=\"text-foreground text-base font-medium\">\n                  $4,791.80\n                </div>\n              </div>\n              <div className=\"flex items-center space-x-4\">\n                <Avatar className=\"h-12 w-12\">\n                  <AvatarImage src=\"data:image/png;base64,\" />\n                  <AvatarFallback>JD</AvatarFallback>\n                </Avatar>\n                <div className=\"flex-1 space-y-1\">\n                  <p className=\"text-foreground text-sm font-medium\">\n                    John Dukes\n                  </p>\n                  <p className=\"text-muted-foreground text-sm\">\n                    johnd@gmail.com\n                  </p>\n                </div>\n                <div className=\"text-foreground text-base font-medium\">\n                  $9,336.08\n                </div>\n              </div>\n              <div className=\"flex items-center space-x-4\">\n                <Avatar className=\"h-12 w-12\">\n                  <AvatarImage src=\"data:image/png;base64,\" />\n                  <AvatarFallback>JR</AvatarFallback>\n                </Avatar>\n                <div className=\"flex-1 space-y-1\">\n                  <p className=\"text-foreground text-sm font-medium\">\n                    Judith Rodriguez\n                  </p>\n                  <p className=\"text-muted-foreground text-sm\">\n                    judith@gmail.com\n                  </p>\n                </div>\n                <div className=\"text-foreground text-base font-medium\">\n                  $8,338.52\n                </div>\n              </div>\n              <div className=\"flex items-center space-x-4\">\n                <Avatar className=\"h-12 w-12\">\n                  <AvatarImage src=\"data:image/png;base64,\" />\n                  <AvatarFallback>RS</AvatarFallback>\n                </Avatar>\n                <div className=\"flex-1 space-y-1\">\n                  <p className=\"text-foreground text-sm font-medium\">\n                    Rodger Struck\n                  </p>\n                  <p className=\"text-muted-foreground text-sm\">\n                    roger@gmail.com\n                  </p>\n                </div>\n                <div className=\"text-foreground text-base font-medium\">\n                  $741.28\n                </div>\n              </div>\n              <div className=\"flex items-center space-x-4\">\n                <Avatar className=\"h-12 w-12\">\n                  <AvatarImage src=\"data:image/png;base64,\" />\n                  <AvatarFallback>AB</AvatarFallback>\n                </Avatar>\n                <div className=\"flex-1 space-y-1\">\n                  <p className=\"text-foreground text-sm font-medium\">\n                    Alex Buckmaster\n                  </p>\n                  <p className=\"text-muted-foreground text-sm\">\n                    alexb23@gmail.com\n                  </p>\n                </div>\n                <div className=\"text-foreground text-base font-medium\">\n                  $1,629.88\n                </div>\n              </div>\n            </CardContent>\n          </Card>\n        </div>\n      </main>\n    </div>\n  );\n}\n",
      "type": "registry:component"
    },
