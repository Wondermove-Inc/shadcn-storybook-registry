    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "prettier": "^3.6.2",
    "radix-ui": "^1.4.3",
    "react": "19.1.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.63.0",
